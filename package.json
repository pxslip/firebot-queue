{
  "name": "firebot-queue",
  "scriptOutputName": "firebot-queue",
  "version": "1.0.0-alpha.1",
  "description": "A starter Firebot custom script",
  "main": "",
  "scripts": {
    "build": "webpack",
    "build:dev": "npm run build && npm run copy",
    "copy": "node ./scripts/copy-build.js",
    "test": "jest",
    "watch:build:dev": "npm-watch build:dev"
  },
  "watch": {
    "build:dev": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,html",
      "quiet": true
    }
  },
  "author": "pxslip",
  "license": "GNU3",
  "devDependencies": {
    "@crowbartools/firebot-custom-scripts-types": "^5.53.2",
    "@types/node": "^16.0.0",
    "html-loader": "^4.1.0",
    "jest": "^29.0.3",
    "npm-watch": "^0.11.0",
    "terser-webpack-plugin": "^5.3.6",
    "ts-jest": "^29.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.8.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}