    "html-loader": "^4.1.0",
    "jest": "^29.0.3",
    "npm-watch": "^0.11.0",
    "postcss": "^8.4.16",
    "postcss-loader": "^7.0.1",
    "postcss-preset-env": "^7.8.2",
    "posthtml": "^0.16.6",
